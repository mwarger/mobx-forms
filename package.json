{
  "name": "mobx-forms",
  "version": "0.0.0",
  "description": "A simple form management for Mobx & React.",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"src/**\"",
    "build": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/mobx-forms.js",
    "build:umd:min": "NODE_ENV=production webpack src/index.js dist/mobx-forms.min.js",
    "build:examples": "webpack examples/index.jsx examples/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oreqizer/mobx-forms.git"
  },
  "keywords": [
    "form",
    "forms",
    "mobx",
    "react"
  ],
  "author": "oreqizer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oreqizer/mobx-forms/issues"
  },
  "homepage": "https://github.com/oreqizer/mobx-forms#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "webpack": "^2.1.0-beta.21"
  },
  "peerDependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "mobx": "^2.4.3",
